        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.13.1": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ws@1.1.1": {
        "dependencies": {
          "options": ">=0.0.5",
          "ultron": "1.0.x",
          "utf-8-validate": "1.2.x",
          "bufferutil": "1.2.x"
        }
      }
    }
