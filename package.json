  "ava": {
    "files": [
      "test/**/*",
      "!node_modules/**/*",
      "!packages/**/*"
    ],
    "babel": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ]
    },
    "require": [
      "babel-polyfill",
      "./test/helpers/ava-jspm-loader.js",
      "./test/client/helpers/emulate-browser.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/test/**/*",
      "packages/**/*"
    ],
    "sourceMap": false,
    "instrument": false,
    "require": [
      "babel-register"
    ],
    "all": false
  }
